{
  "name": "backend-portfolio",
  "version": "1.0.0",
  "main": "src/server.ts",
  "license": "MIT",
  "scripts": {
    "tsc": "tsc",
    "start:dev": "NODE_ENV=development nodemon",
    "start:production": "NODE_ENV=production && yarn tsc",
    "prettier:check": "prettier --config .prettierrc.js --check \"*/**/*.{ts,tsx,js,jsx,json,sass,scss,css,html,md}\"",
    "prettier:fix": "yarn prettier:check --write",
    "prettier:fix-all": "yarn prettier:check --write",
    "precommit:check": "yarn prettier:fix-all",
    "heroku-postbuild": "NODE_ENV=production && yarn install && yarn tsc"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn prettier:fix"
    }
  },
  "dependencies": {
    "@types/component-emitter": "^1.2.10",
    "@types/cookie": "^0.4.0",
    "@types/cors": "^2.8.9",
    "@types/mongoose": "^5.10.3",
    "@types/node": "^14.14.20",
    "@types/nodemailer": "^6.4.0",
    "@types/nodemailer-mailgun-transport": "^1.4.2",
    "@types/socket.io": "^2.1.12",
    "@types/yup": "^0.29.11",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-expeditious": "^5.1.1",
    "express-graphql": "^0.12.0",
    "global": "^4.4.0",
    "graphql": "^15.4.0",
    "husky": "^4.3.7",
    "mongoose": "5.10.15",
    "nodemailer": "^6.4.17",
    "nodemailer-mailgun-transport": "^2.0.2",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "socket.io": "^3.0.5",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3",
    "yup": "^0.32.8"
  }
}